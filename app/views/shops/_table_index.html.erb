<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Retailer</th>
    </tr>
  </thead>
  <tbody>
    <% @shops.each do |shop| %>
      <tr>
        <td><%= shop.name %></td>
        <td><%= shop.email %></td>
        <td><%= shop.retailer %></td>
      </tr>
    <% end %>
  </tbody>
</table>